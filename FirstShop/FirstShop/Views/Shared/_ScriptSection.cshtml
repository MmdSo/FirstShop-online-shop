<script>
        function successRegister(result){
            if(result == 0)
            ShowToastr("error", "please fill the form corectly");
            if(result == 1)
            ShowToastr("warning", "User name is taken");
            if(result == 2)
            ShowToastr("warning", "the password should be 6 charecter or higher !");
            if(result == 3)
            ShowToastr("warning", "confirm password dosent match to the password");
            if(result == 4)
            ShowToastr("warning", "please fill the confirm password field");
            if(result == 10)
            {
                ShowToastr("success", "Your regestration are successfull!");
                window.setTimeout(function(){
                    window.location.replace("/");
                },5000);
            }
            if(result == 100);
            ShowToastr("error", "Error ecured");
        }

        function successLogin(result){
            if(result == 0)
                ShowToastr("error", "please fill the form corectly");
            else if(result == 2)
                ShowToastr("warning", "User not exist or input info Error!");
            else if(result == 10)
            {
                ShowToastr("success", "Successful Login");
                window.setTimeout(function(){
                    window.location.replace("/");
                },5000);
            }
            else
            ShowToastr("error", "Error ecured");
        }
        

        function ShowToastr(type, message){
            toastr.options = {
                "debug" : false,
                "positionClass" : "toast-bottom-left",
                "progressBar" : true,
                "onClick" : null,
                "fadeIn" : 300,
                "fadeOut" : 1000,
                "timeOut" : 5000
            };
            if(type === "success")
                toastr.success(message,'success');
            if(type === "warning")
                toastr.warning(message,'warning');
            if(type === "error")
                toastr.error(message,'error');
            if(type === "info")
                toastr.info(message,'info');
        }



